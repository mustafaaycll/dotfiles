# Custom zsh
[ -f "$HOME/.dotfiles/zsh/custom.zsh" ] && source "$HOME/.dotfiles/zsh/custom.zsh"

# Aliases
[ -f "$HOME/.dotfiles/zsh/aliases.zsh" ] && source "$HOME/.dotfiles/zsh/aliases.zsh"

# Options
[ -f "$HOME/.dotfiles/zsh/options.zsh" ] && source "$HOME/.dotfiles/zsh/options.zsh"

# Bindings
[ -f "$HOME/.dotfiles/zsh/bindings.zsh" ] && source "$HOME/.dotfiles/zsh/bindings.zsh"